<p style="color: green"><%= notice %></p>

<h1>Transactions</h1>

<div id="transactions">
  <% @transactions.each do |transaction| %>
    <%= render transaction %>
    <p>
      <%= link_to "Show this transaction", company_transaction_path(@company, transaction) %>
    </p>
  <% end %>
</div>

<%= link_to "Back to company", company_path(@company, @transaction) %>
